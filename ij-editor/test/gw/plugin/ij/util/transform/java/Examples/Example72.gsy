package gw.util.transform.java.Examples

uses java.lang.*

class Example72  {

    public static function main(args : String[]) : void {
        System.out.println("Thursday is " + wdayno2("Thursday"))
    }

    static function wdayno2(wday : String) : int {
        var i = 0
        while (i < wdays.length and !wday.equals(wdays[i])) {
            i++
        }

        if (i < wdays.length) {
            return i + 1
        } else {
            return -1
        }
    }

    static final internal var wdays : String[] = {"Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"}

}
