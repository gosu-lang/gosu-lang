package gw.util.transform.java.Examples

uses java.lang.*

class Example68  {

    public static function main(args : String[]) : void {
        var i = 1
        while (i <= 4) {
            var j = 1
            while (j <= i) {
                System.out.print("*")
                j++
            }

            System.out.println()
            i++
        }

    }

}
