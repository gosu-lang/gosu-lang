package gw.util.transform.java.Examples

uses java.lang.*

class Example2  {

    internal var x : int
    construct(x_0 : int) {
        this.x = x_0
    }

    function sum(y : int) : int {
        if (x > 0) {
            return x + y
        } else if (x < 0) {
            var res : int = -x + y
            return res * 117
        } else {
            var sum = 0
            for (i in 0..|10) {
                sum += (y - i) * (y - i)
            }

            return sum
        }
    }

