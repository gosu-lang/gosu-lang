package gw.util.transform.java.Examples

uses java.lang.*

class Example51  {

    public static function main(args : String[]) : void {
        System.out.println(absolute(-12))
        System.out.println(absolute(12))
        System.out.println(absolute(0))
    }

    static function absolute(x : double) : double {
        return (x >= 0 ? x : -x)
    }

}
