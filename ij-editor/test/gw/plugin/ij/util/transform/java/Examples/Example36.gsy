package gw.util.transform.java.Examples

uses java.lang.*

class Point  {

    internal var x : int
    internal var y : int
    construct(x_0 : int, y_0 : int) {
        this.x = x_0
        this.y = y_0
    }

    construct(p : Point) {
        this(p.x, p.y)
    }

    function move(dx : int, dy : int) : void {
        x += dx
        y += dy
    }

    public function toString() : String {
        return "(" + x + ", " + y + ")"
    }

}
