  function testRationalPByteDivision() {
    assertThrowsArithmeticException("Division undefined", \ -> rational("0") / p_byte(0))
    assertEquals(rational("0"), rational("0") / p_byte(1))
    assertEquals(rational("0"), rational("0") / p_byte(-1))
    assertEquals(rational("0"), rational("0") / p_byte(23))
    assertEquals(rational("0"), rational("0") / p_byte(-32))
    assertEquals(rational("0"), rational("0") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("0"), rational("0") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("1") / p_byte(0))
    assertEquals(rational("1"), rational("1") / p_byte(1))
    assertEquals(rational("-1"), rational("1") / p_byte(-1))
    assertEquals(rational("1 / 23"), rational("1") / p_byte(23))
    assertEquals(rational("-0.03125"), rational("1") / p_byte(-32))
    assertEquals(rational("1 / 127"), rational("1") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("-0.0078125"), rational("1") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("-1") / p_byte(0))
    assertEquals(rational("-1"), rational("-1") / p_byte(1))
    assertEquals(rational("1"), rational("-1") / p_byte(-1))
    assertEquals(rational("-1 / 23"), rational("-1") / p_byte(23))
    assertEquals(rational("0.03125"), rational("-1") / p_byte(-32))
    assertEquals(rational("-1 / 127"), rational("-1") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("0.0078125"), rational("-1") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("23") / p_byte(0))
    assertEquals(rational("23"), rational("23") / p_byte(1))
    assertEquals(rational("-23"), rational("23") / p_byte(-1))
    assertEquals(rational("1"), rational("23") / p_byte(23))
    assertEquals(rational("-0.71875"), rational("23") / p_byte(-32))
    assertEquals(rational("23/127"), rational("23") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("-0.1796875"), rational("23") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("23.123") / p_byte(0))
    assertEquals(rational("23.123"), rational("23.123") / p_byte(1))
    assertEquals(rational("-23.123"), rational("23.123") / p_byte(-1))
    assertEquals(rational("23123 / 23000"), rational("23.123") / p_byte(23))
    assertEquals(rational("-0.72259375"), rational("23.123") / p_byte(-32))
    assertEquals(rational("23123 / 127000"), rational("23.123") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("-0.1806484375"), rational("23.123") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("-32") / p_byte(0))
    assertEquals(rational("-32"), rational("-32") / p_byte(1))
    assertEquals(rational("32"), rational("-32") / p_byte(-1))
    assertEquals(rational("-32 / 23"), rational("-32") / p_byte(23))
    assertEquals(rational("1"), rational("-32") / p_byte(-32))
    assertEquals(rational("-32 / 127"), rational("-32") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("0.25"), rational("-32") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("-32.456") / p_byte(0))
    assertEquals(rational("-32.456"), rational("-32.456") / p_byte(1))
    assertEquals(rational("32.456"), rational("-32.456") / p_byte(-1))
    assertEquals(rational("-4057 / 2875"), rational("-32.456") / p_byte(23))
    assertEquals(rational("1.01425"), rational("-32.456") / p_byte(-32))
    assertEquals(rational("-4057 / 15875"), rational("-32.456") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("0.2535625"), rational("-32.456") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("123456789012345678901234567890.123456789") / p_byte(0))
    assertEquals(rational("123456789012345678901234567890.123456789"), rational("123456789012345678901234567890.123456789") / p_byte(1))
    assertEquals(rational("-123456789012345678901234567890.123456789"), rational("123456789012345678901234567890.123456789") / p_byte(-1))
    assertEquals(rational("123456789012345678901234567890123456789 / 23000000000"), rational("123456789012345678901234567890.123456789") / p_byte(23))
    assertEquals(rational("-123456789012345678901234567890123456789 / 32000000000"), rational("123456789012345678901234567890.123456789") / p_byte(-32))
    assertEquals(rational("123456789012345678901234567890123456789 / 127000000000"), rational("123456789012345678901234567890.123456789") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("-123456789012345678901234567890123456789 / 128000000000"), rational("123456789012345678901234567890.123456789") / p_byte(Byte.MIN_VALUE))

    assertThrowsArithmeticException("Division by zero", \ -> rational("-123456789012345678901234567890.123456789") / p_byte(0))
    assertEquals(rational("-123456789012345678901234567890.123456789"), rational("-123456789012345678901234567890.123456789") / p_byte(1))
    assertEquals(rational("123456789012345678901234567890.123456789"), rational("-123456789012345678901234567890.123456789") / p_byte(-1))
    assertEquals(rational("-123456789012345678901234567890123456789 / 23000000000"), rational("-123456789012345678901234567890.123456789") / p_byte(23))
    assertEquals(rational("123456789012345678901234567890123456789 / 32000000000"), rational("-123456789012345678901234567890.123456789") / p_byte(-32))
    assertEquals(rational("-123456789012345678901234567890123456789 / 127000000000"), rational("-123456789012345678901234567890.123456789") / p_byte(Byte.MAX_VALUE))
    assertEquals(rational("123456789012345678901234567890123456789 / 128000000000"), rational("-123456789012345678901234567890.123456789") / p_byte(Byte.MIN_VALUE))

    assertEquals(Rational, statictypeof(rational("0") / p_byte(0)))
  }

