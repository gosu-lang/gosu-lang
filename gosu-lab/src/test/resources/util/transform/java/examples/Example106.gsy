package gw.util.transform.java.Examples

uses java.util.Date

class Pair<T, U>  {

    public final var fst : T
    public final var snd : U
    public construct(fst_0 : T, snd_0 : U) {
        this.fst = fst_0
        this.snd = snd_0
    }

}
class Example106  {

    public static function main(args : String[]) : void {
        var p1 = new Pair<String, Integer>("Niels", 1947)
        var p2 = new Pair<Double, Integer>(2.718, 1)
        var p3 = new Pair<Date, String>(new Date(), "now")
        System.out.println("(" + p1.fst + ", " + p1.snd + ")")
        System.out.println("(" + p2.fst + ", " + p2.snd + ")")
        System.out.println("(" + p3.fst + ", " + p3.snd + ")")
    }

}
