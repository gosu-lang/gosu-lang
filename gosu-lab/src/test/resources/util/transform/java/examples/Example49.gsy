package gw.util.transform.java.Examples

uses java.lang.*

class Example49  {

    public static function main(args : String[]) : void {
        var p1 = new Point(10, 20)
        System.out.println("p1 is " + p1)
        var p2 : Point = p1
        p2.move(8, 8)
        System.out.println("p2 is " + p2)
        System.out.println("p1 is " + p1)
    }

}
