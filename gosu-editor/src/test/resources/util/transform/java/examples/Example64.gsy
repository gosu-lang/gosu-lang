package gw.util.transform.java.Examples

uses java.lang.*

class Example64  {

    public static function main(args : String[]) : void {
        System.out.println("Thursday is " + wdayno1("Thursday"))
    }

    static function wdayno1(wday : String) : int {
        if (wday.equals("Monday")) {
            return 1
        } else if (wday.equals("Tuesday")) {
            return 2
        } else if (wday.equals("Wednesday")) {
            return 3
        } else if (wday.equals("Thursday")) {
            return 4
        } else if (wday.equals("Friday")) {
            return 5
        } else if (wday.equals("Saturday")) {
            return 6
        } else if (wday.equals("Sunday")) {
            return 7
        } else {
            return -1
        }
    }

}
